{
  "name": "expo-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "private": true,
  "scripts": {
    "start": "APP_ENV=development bunx expo start --dev-client -c",
    "android": "bunx expo run:android",
    "ios": "bunx expo run:ios",
    "check-types": "tsc --noEmit",
    "prebuild": "bunx expo prebuild",
    "build:android:dev": "eas build --profile development  --platform android ",
    "build:ios:dev": "eas build --profile development  --platform ios",
    "build:all:dev": "eas build --profile development --platform all",
    "build:android:dev-preview": "eas build --profile dev-preview  --platform android",
    "build:ios:dev-preview": "eas build --profile dev-preview  --platform ios",
    "build:all:dev-preview": "eas build --profile dev-preview --platform all",
    "build:android:preview": "eas build --profile dev-preview  --platform android",
    "build:ios:preview": "eas build --profile preview  --platform ios",
    "build:all:preview": "eas build --profile preview --platform all",
    "build:android:prod": "eas build --profile dev-production  --platform android",
    "build:ios:prod": "eas build --profile production  --platform ios",
    "update:all:dev": "eas update --branch dev",
    "update:all:dev-preview": "eas update --branch dev-production",
    "update:all:prod": "eas update --branch production",
    "analyze:web": "bunx source-map-explorer 'dist/_expo/static/js/web/*.js' 'dist/_expo/static/js/web/*.js.map'",
    "analyze:ios": "bunx source-map-explorer 'dist/_expo/static/js/ios/*.js' 'dist/_expo/static/js/ios/*.js.map'",
    "analyze:android": "bunx source-map-explorer 'dist/_expo/static/js/android/*.js' 'dist/_expo/static/js/android/*.js.map'"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@core/ui": "0.0.1",
    "@expo/config-plugins": "~7.8.0",
    "@react-navigation/native": "^6.1.6",
    "app": "0.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "burnt": "^0.12.2",
    "expo": "~50.0.13",
    "expo-constants": "~15.4.5",
    "expo-dev-client": "~3.3.11",
    "expo-font": "~11.10.3",
    "expo-linear-gradient": "~12.7.2",
    "expo-linking": "~6.2.2",
    "expo-localization": "~14.8.3",
    "expo-router": "~3.4.8",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-updates": "~0.24.12",
    "i18next": "^23.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-keyboard-controller": "^1.11.3",
    "react-native-mmkv": "^2.12.1",
    "react-native-reanimated": "^3.8.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@expo/metro-config": "~0.17.1",
    "@tamagui/babel-plugin": "^1.92.0",
    "metro-minify-terser": "^0.74.1",
    "typescript": "^5.3.3"
  }
}
