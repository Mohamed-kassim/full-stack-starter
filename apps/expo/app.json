{
  "expo": {
    "name": "yourprojectsname",
    "slug": "yourprojectsname",
    "scheme": "yourprojectsname",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.yourprojectsname.app"
      // blockedPermissions: ['android.permission.RECORD_AUDIO'],
      // permissions: ['android.permission.CAMERA'],
    },
    "locales": {
      "ar": "./src/localization/translations/ar/locales.json",
      "en": "./src/localization/translations/en/locales.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization"
      // [
      //   "expo-camera",
      //   {
      //     "cameraPermission": "Allow $(PRODUCT_NAME) to access camera to scan products barcode."
      //   }
      // ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "226fdba3-18a3-45a3-8b62-48175555f2fc"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
